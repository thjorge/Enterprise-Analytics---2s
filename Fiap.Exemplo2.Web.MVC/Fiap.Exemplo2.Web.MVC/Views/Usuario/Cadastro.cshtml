
@{
    ViewBag.Title = "Cadastro";
}

<h2>Cadastro de Usuário</h2>

<form action="/Usuario/Cadastro" method="post">
      <div class="form-group">
          <label for="id-nome">Nome</label>
          <input type="text" nome="nome" id="id-nome" class="form-control" />
      </div>
    <div class="form-group">
        <label for="id-idade">Idade</label>
        <input type="text" nome="idade" id="id-idade" class="form-control" />
    </div>
    <div class="form-group">
        <label for="id-email">E-mail</label>
        <input type="email" nome="email" id="id-email" class="form-control" />
    </div>
    <input type="submit" value="Cadastrar" class="btn btn-success"/>
</form>

